import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        // Write your code here
        int count=0;
        Scanner scan=new Scanner(System.in);
        int l=scan.nextInt();
        int r=scan.nextInt();
        int k=scan.nextInt();

        for(int i=l;i<=r;i++){
            if(i%k==0){
                count++;
            }
        }
        System.out.println(count);
    }
}
